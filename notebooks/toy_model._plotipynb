    "import networkx as nx\n",
    "from src.utils.utils_networks import favor_centrality\n",
    "import matplotlib.pyplot as plt\n",
    "import os\n",
    "from pathlib import Path\n",
    "from itertools import count"
   ]
  },
  {
   "cell_type": "code",
